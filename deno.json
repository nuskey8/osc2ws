{
  "name": "@nuskey8/osc2ws",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Wraps local OSC output as a WebSocket server",
  "exports": "./main.ts",
  "imports": {
    "@nuskey8/osc-wasm": "jsr:@nuskey8/osc-wasm@^0.1.4",
    "@std/assert": "jsr:@std/assert@1",
    "@std/flags": "jsr:@std/flags@^0.224.0"
  },
  "tasks": {
    "start": "deno run -A --unstable-net main.ts --osc-host 127.0.0.1 --osc-port 8001 --ws-host localhost --ws-port 8080 -v"
  }
}
